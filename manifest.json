{
  "name": "Wordle Solver",
  "description": "Helps solve the wordle game.",
  "version": "1.0",
  "manifest_version": 3,
  "minimum_chrome_version": "93",
  "background": {
    "service_worker": "src/chrome-scripts/background.js",
    "type": "module"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "optional_permissions": ["tabs"],
  "host_permissions": ["*://*.nytimes.com/games/wordle/index.html"],
  "content_scripts": [
    {
      "matches": ["*://*.nytimes.com/games/wordle/index.html"],
      "js": ["src/chrome-scripts/contentServices/content.js"],
      "run_at": "document_end"
    }
  ],
  "action": {
    "default_icon": {
      "16": "/images/get_started16.png",
      "32": "/images/get_started32.png",
      "48": "/images/get_started48.png",
      "128": "/images/get_started128.png"
    }
  },
  "icons": {
    "16": "/images/get_started16.png",
    "32": "/images/get_started32.png",
    "48": "/images/get_started48.png",
    "128": "/images/get_started128.png"
  },
  "web_accessible_resources": [
  ]
}